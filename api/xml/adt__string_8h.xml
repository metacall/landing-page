        <name>string_copy</name>
        <param><defname>dest</defname></param>
        <param><defname>src</defname></param>
        <param><defname>dest_size</defname></param>
        <initializer>	do \
	{ \
		size_t __string_copy_length = strnlen(src, dest_size - 1); \
		memcpy(dest, src, __string_copy_length); \
		dest[__string_copy_length] = &apos;\0&apos;; \
